{
    "rust-analyzer.cargo.allTargets": false,
    "rust-analyzer.cargo.target": "thumbv7m-none-eabi",
    "rust-analyzer.cargo.features": [
        "critical-section-single-core",
        "f103",
        "xG",
    ],
    "editor.formatOnSave": true,
    "files.exclude": {
        "**/.mypy_cache": true,
        "**/__pycache__": true,
    }
}
